void but1void() {
  if ( mouseX>=butX1 && mouseX<=butX1+butWidth && mouseY>=butY1 && mouseY<=butY1+butHeight ) {
    fill(hoverColor);
    rect(butX1, butY1, butWidth, butHeight);
  } else {
    fill(buttonColor);
    rect(butX1, butY1, butWidth, butHeight);
  }
  fill(white);
}//end of but1void

